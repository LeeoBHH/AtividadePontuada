programa {
  funcao inicio() {
//Declarando variaveis
inteiro valor1, valor2, valor3, soma

//Coletando dados
escreva("Digite o valor: ")
leia(valor1)
escreva("Digite o valor: ")
leia(valor2)
escreva("Digite o valor: ")
leia(valor3)

//Calculando
soma = valor1 + valor2
se (soma < valor3) escreva("\nA soma é menor que o valor 3")
       senao escreva("\nA soma é maior que o valor 3")

//Exibindo resultados
  }
}
