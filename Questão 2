programa {
  funcao inicio() {
//Declarando variavieis
cadeia nome, estado_civil, casada, feminino, tempo
caracter sexo



//Coletando dados
escreva("Digite seu nome: ")
leia(nome)
escreva("Digite seu estado cívil: ")
leia(estado_civil)
escreva("Digite seu sexo: ")
leia(sexo)

//Calculando
se (sexo == 'f' e estado_civil == "casada")
{
escreva("\nTempo de casada: ")
leia(tempo)
escreva("\nDados disponibilizados: ")
escreva("\nNome: " + nome)
escreva("\nEstado Cívil: " + estado_civil)
escreva("\nSexo: " + sexo)
escreva("\nTempo de casada: " + tempo)
}senao{ 
escreva ("\n----FIM----")     

}
                                   



//Exibindo resultados-



  }
}
