programa {
  funcao inicio() {
//Declarando variaveis
inteiro valor1, valor2, soma, multiplicacao

//Coletando dados
escreva("Digite o primeiro valor: ")
leia(valor1)
escreva("Digite o segundo valor: ")
leia(valor2)


//Calculando
soma = valor1 + valor2
multiplicacao = valor1 * valor2

se (valor1 == valor2)
escreva("A soma dos valores é igual a: " + soma)
 senao escreva("A multiplicação dos valores é igual a: " + multiplicacao)

//Exibindo resultados
  }
}
