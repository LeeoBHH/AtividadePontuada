programa {
  funcao inicio() {
//Declarando variaveis
cadeia cd
real verde, azul, amarelo, vermelho
//Coletando dados
escreva("Digite a cor para visualizar o preço do CD: ")
leia(cd)

//Calculando
se (cd == "verde") escreva("O valor é R$10.00!")
se (cd == "azul") escreva("O valor é R$20.00!")
se (cd == "amarelo") escreva("O valor é R$30.00!")
se (cd == "vermelho") escreva("O valor é R$40.00!")

//Exibindo resultados


  }
}
